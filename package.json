{
  "name": "rpa-runner",
  "version": "4.2.2",
  "main": "dist/main/main.js",
  "description": "AutoWork 让工作变简单",
  "scripts": {
    "dev:ui": "cross-env NODE_ENV=development webpack-dev-server --config config/ui/webpack.dev.js",
    "dev:main": "cross-env NODE_ENV=development electron-webpack dev",
    "build:ui": "webpack --config config/ui/webpack.prod.js",
    "build:main": "webpack --config config/main/webpack.prod.js",
    "dist": "electron-builder --ia32 --config electron-builder.json",
    "release:dev": "cross-env NODE_ENV=development yarn build:main && yarn build:ui && yarn dist",
    "release:prod": "cross-env NODE_ENV=production yarn build:main && yarn build:ui && yarn dist",
    "prebuild": "svn update ./static/awrunner",
    "format": "prettier --write .",
    "postinstall": "electron-builder install-app-deps --arch=ia32"
  },
  "keywords": [],
  "author": "xieyuh",
  "license": "ISC",
  "electronWebpack": {
    "main": {
      "sourceDirectory": "src",
      "webpackConfig": "config/main/webpack.dev.js"
    },
    "renderer": {
      "sourceDirectory": null
    }
  },
  "homepage": "https://www.autoworkrpa.com/",
  "devDependencies": {
    "@babel/core": "^7.11.0",
    "@babel/preset-react": "^7.10.4",
    "@babel/preset-typescript": "^7.10.4",
    "@types/cookie": "^0.4.0",
    "@types/crypto-js": "^4.0.1",
    "@types/ip": "^1.1.0",
    "@types/lodash": "^4.14.164",
    "@types/md5": "^2.2.0",
    "@types/node": "12.12.6",
    "@types/node-rsa": "^1.0.0",
    "@types/node-schedule": "^1.3.0",
    "@types/p-retry": "^3.0.1",
    "@types/qs": "^6.9.3",
    "@types/react-dom": "^16.9.8",
    "@types/react-router-dom": "^5.1.5",
    "@types/request": "^2.48.5",
    "@types/sqlite3": "^3.1.6",
    "@types/websocket": "^1.0.0",
    "@types/ws": "^7.2.6",
    "antd": "^4.5.2",
    "axios": "^0.19.2",
    "babel-loader": "^8.1.0",
    "babel-plugin-import": "^1.13.0",
    "clean-webpack-plugin": "^3.0.0",
    "copy-webpack-plugin": "^6.0.2",
    "cross-env": "^7.0.2",
    "css-loader": "^3.6.0",
    "dotenv": "^8.2.0",
